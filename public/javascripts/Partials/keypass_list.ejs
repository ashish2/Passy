<div class="col-sm-12">
	<h4 id="page-title">{{data.title}} </h4>
	<form action="/keyspass/check" method="post" accept-charset="utf-8">
		<div id="list">
				<div class="item" data-ng-repeat="sq in data.sq">
					<a class="update-link" href="/keyspass/edit/{{sq._id}}" title="Update this sa item">
						{{sq.question }}
					</a>
					&nbsp;
					<input type="text" name="ans[{{sq._id }}]">
				</div>
		</div>
		<input type="submit">
	</form>
</div>

